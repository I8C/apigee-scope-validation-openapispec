<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<SharedFlow name="default">
    <Step>
        <Name>AM-Create-Local-Variable</Name>
    </Step>
    <Step>
        <Name>KVM-Lookup-Scope-Mapping</Name>
    </Step>
    <Step>
        <Name>RF-KVM-Value-Missing</Name>
        <Condition>(SF-Lib-Scope-Validation.scope-mapping == null) or (SF-Lib-Scope-Validation.scope-mapping == "")</Condition>
    </Step>
    <Step>
        <Name>JS-Scope-Checks</Name>
    </Step>
    <Step>
        <Name>RF-Invalid-Scopes</Name>
        <Condition>(SF-Lib-Scope-Validation.valid == false)</Condition>
    </Step>
</SharedFlow>